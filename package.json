{
  "name": "brave-extension",
  "version": "1.0.0",
  "description": "UI prototype in Chromium",
  "scripts": {
    "dev": "node scripts/dev",
    "build": "node scripts/build",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "lint": "standard",
    "test-e2e": "cross-env NODE_ENV=test mocha ./test/setupApp test/e2e/braveShieldsPanel && mocha ./test/setupApp test/e2e/newTabPage.js",
    "test": "cross-env NODE_ENV=test mocha -r ./test/setupApp test/app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhen0409/react-chrome-extension-boilerplate.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "react",
    "redux",
    "hot reload",
    "webpack",
    "boilerplate"
  ],
  "standard": {
    "globals": [
      "chrome",
      "__webpack_hash__",
      "__webpack_require__",
      "__webpack_public_path__",
      "$hotChunkFilename$"
    ],
    "ignore": [
      "build/",
      "dev/"
    ]
  },
  "pre-push": {
    "run": [
      "lint"
    ]
  },
  "author": "Brian Bondy <netzen@gmail.com>",
  "license": "MPL-2.0",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chromedriver": "^2.33.2",
    "cross-env": "^5.1.1",
    "crx": "^3.2.1",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "jsdom": "^11.4.0",
    "minimist": "^1.2.0",
    "mocha": "^4.0.1",
    "postcss-loader": "^2.0.8",
    "pre-push": "^0.1.1",
    "pug-cli": "^1.0.0-alpha6",
    "react-addons-test-utils": "^15.6.2",
    "rimraf": "^2.6.2",
    "selenium-webdriver": "^3.6.0",
    "shelljs": "^0.7.8",
    "sinon": "^4.1.2",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-httpolyglot-server": "^0.2.2"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "brave-ui": "^0.2.1",
    "classnames": "^2.2.5",
    "deep-freeze-node": "^1.1.3",
    "react": "^16.1.1",
    "react-chrome-redux": "^1.4.0",
    "react-dock": "^0.2.4",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
